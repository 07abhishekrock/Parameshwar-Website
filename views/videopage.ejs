  <div class="album">
    <%videos.forEach((video)=>{%>
        <div class="video-card">
          <img src="/img/<%=video.photo%>"/>
          <div class="video-info">
            <h3><%=video.name.length > 40 ? video.name.slice(0,37) + '...' : video.name%></h3>
            <div class="video-sub-info">
              <a href="/videos/<%=video._id%>"><i class="fas fa-play-circle"></i>Play Now</a>
