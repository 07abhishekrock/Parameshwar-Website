<div class="modal myModal" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
	  <div class="modal-content">
		 <div class="modal-header">
			<h5 class="modal-title" id="exampleModalLabel">Register As Subscriber</h5>
			<button type="button" id="modal-close-btn" class="close" data-dismiss="modal" aria-label="Close" onclick="closeModal()">
			  <span aria-hidden="true">&times;</span>
			</button>
		 </div>
		 <div class="modal-body">
			  <div class="form-group">
				 <label for="subscriber-name-field">Name</label>
				 <input type="text" class="form-control" id="subscriber-name-field" aria-describedby="name field" placeholder="Enter name here" required>
				 <small class="form-text text-muted">Error goes here.</small>
			  </div>
			  <div class="form-group">
				 <label for="subscriber-email-field">Email</label>
				 <input type="email" class="form-control" id="subscriber-email-field" placeholder="Enter Email Here" required>
				 <small class="form-text text-muted">Error goes here.</small>
			  </div>
			  <div class="form-group">
				 <label for="subscriber-phoneNo-field">Phone Number</label>
				 <input type="text" class="form-control" id="subscriber-phoneNo-field" placeholder="Enter PhoneNo here" required>
				 <small class="form-text text-muted">Error goes here.</small>
			  </div>
			  </div>
			  <button id="register-submit-btn" class="btn btn-primary">Register Now</button>
		 </div>
	  </div>
	</div>